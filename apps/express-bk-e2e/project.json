{
  "name": "@express-bk/express-bk-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "implicitDependencies": [
    "@express-bk/express-bk"
  ],
  "targets": {
    "e2e": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{e2eProjectRoot}"
      ],
      "options": {
        "jestConfig": "apps/express-bk-e2e/jest.config.ts",
        "passWithNoTests": true
      },
      "dependsOn": [
        "@express-bk/express-bk:build",
        "@express-bk/express-bk:serve"
      ]
    }
  }
}
